for j=1,13 do
for i=1,6 do
    turtle.digUp()
    turtle.digDown()
    turtle.dig()
    turtle.forward()
end
turtle.turnLeft()
turtle.dig()
turtle.forward()
turtle.turnLeft()
for i=1,6 do
    turtle.digUp()
    turtle.digDown()
    turtle.dig()
    turtle.forward()
end
turtle.turnRight()
turtle.dig()
turtle.forward()
turtle.turnRight()
end
